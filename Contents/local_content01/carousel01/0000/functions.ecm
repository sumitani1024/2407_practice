// Hybridcast対応判定(0:非対応/1:フェーズ0対応)
function isHC(){return(browser.getBrowserSupport('ARIB','APIGroup','AITControlledApp.Start')==1&&browser.getBrowserSupport("ARIB","AITControlledAppEngineFunction","IPTV-F","HTML5_ph0")==1&&browser.getBrowserSupport('ARIB','AITTransportMethod','XML','HTTP')==1)?1:0;}
// Hybridcast遷移
function luHC(c_appid,c_uri){browser.startAITControlledApp("10",c_appid,c_uri);}
// PPP判定
function isPPP(){if(browser.getBrowserSupport("ARIB","APIGroup","Com.IP.Params")){var c_p=browser.getISPParams();return (c_p&&c_p[1]&&c_p[3]&&c_p[4]);}return false;}
// BS判定
function isBS(){if(browser.getBrowserSupport("ARIB","APIGroup","Ctrl.Basic"))return browser.getProgramID(3)=="0x0004";return true;}
// 地上受信可能判定
function canT(){if(browser.getBrowserSupport("ARIB","APIGroup","Ctrl.Media"))return browser.isSupportedMedia("4");return false;}
// ネットワークID取得
function getNID(){return browser.getBrowserSupport("ARIB","APIGroup","Ctrl.Basic")?num0(browser.getProgramID(3).substring(2).toLowerCase(),4):"0004";}
// ブラウザーバージョン取得(フルサイズ)
function getBrowser(){if(browser.getBrowserSupport("ARIB","APIGroup","Ctrl.Version")){var c_r=browser.getBrowserVersion();if(c_r&&c_r.length==4)return c_r[0]+(c_r[1]+"____________________").substring(0,20)+c_r[2]+c_r[3];}return "____________________________";}
// Foliage判定(getBrowserが必要)
function isFoliage(){return getBrowser().substring(0,13)=="00FOLIAGETYPE";}
// 数値を"0"で桁数指定パディング
function num0(c_n,c_c){var c_r="0000000000000000"+c_n;return c_r.substring(c_r.length-c_c);}
// 数値を" "で桁数指定パディング
function nums(c_n,c_c){var c_r="          "+c_n;return c_r.substring(c_r.length-c_c);}
// 1桁の数値を全角化
function numz(c_n){if(c_n<10)return "０１２３４５６７８９".charAt(c_n);return c_n;}
// 【ACASに対応していないため、削除】B-CAS番号を10進数20桁の文字列で取得
