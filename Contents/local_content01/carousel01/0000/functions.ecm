// Ureg[50]にフラグを設定
function setUreg50(c_c,c_s){var c_ar=browser.Ureg[50].split("_");for(var c_l=0;c_l<c_ar.length;c_l++)if(c_ar[c_l].charAt(0)==c_c){c_ar[c_l]=c_c+c_s;browser.Ureg[50]=c_ar.join("_");return;}if(c_ar.length)browser.Ureg[50]+="_";browser.Ureg[50]+=c_c+c_s;}
// Ureg[50]のフラグを取得(未登録がわかるようにフラグ自体も返す)
function getUreg50(c_c){var c_ar=browser.Ureg[50].split("_");for(var c_l=0;c_l<c_ar.length;c_l++)if(c_ar[c_l].charAt(0)==c_c)return c_ar[c_l];return "";}
// UTS送信(getBrowser()が必要)
function us(u_url,c_tkt,c_type,c_ar,c_url){if(u_url.length){var now=new Date();var denbun=c_tkt+":"+(now==null?"00000000000000":now.toString().split("T").join("").split("-").join("").split(":").join(""))+":0000:0:E:"+c_type+":"+c_ar.join(":");if(now!=null&&c_tkt.length==16&&c_type.length<=8&&c_type.length){var r=browser.transmitTextDataOverIP(u_url,denbun,"EUC-JP");if(getBrowser().substring(0,2)=="19")browser.sleep(300);if(Number(r[0])==1&&Number(r[1])==200&&String(r[2]).substring(0,5)=="0000:")return r[2];denbun+="/"+r[1]+"/"+r[2];}if(c_url.length){browser.transmitTextDataOverIP(c_url,denbun+"/"+getBrowser(),"EUC-JP");if(getBrowser().substring(0,2)=="19")browser.sleep(300);}}return"";}
// 文字列の長さ(全角を2文字としてカウント)
function strlen(c_s){var c_n=0;for(var c_l=0;c_l<c_s.length;c_l++)c_n+=c_s.charCodeAt(c_l)>0xff?2:1;return c_n;}
// 文字列の先頭から指定した文字を取得(全角を2文字としてカウント)
function strL(c_s,c_n){var c_m=0;for(var c_l=0;c_l<c_s.length;c_l++){c_m+=c_s.charCodeAt(c_l)>0xff?2:1;if(c_m>c_n)return c_s.substring(0,c_l);}return c_s;}
// シンプルデバッグ(strLが必要)
var debug=browser.Ureg[53]=="debug"?1:0;
var ard=new Array();
function dbg(c_s){if(debug){var c_n=parseInt(s("debug").height)/20;var c_c=String(c_s);while(c_c.length){ard[debug-1]=strL(c_c,parseInt(s("debug").width)/10);c_c=c_c.substring(ard[debug-1].length);debug=(debug)%c_n+1;}var c_p="";for(var c_m=ard.length-1;c_m>=0;c_m--)c_p+=ard[(debug-2-c_m+c_n)%c_n]+"\r\n";p("debug",c_p);}}
// デバッグ(複数id可)
function DEBUG(c_id){this.id=c_id;this.f=false;this.ar=new Array();this.ln=parseInt(s(c_id).height)/16;for(var c_n=0;c_n<=this.ln;c_n++)this.ar[c_n]="";this.po=0;this.flg=mDEBUGf;this.prn=mDEBUGp;}
function mDEBUGf(c_f){this.f=c_f;v(this.id,c_f?1:0);}
function mDEBUGp(c_s){if(this.f){this.ar[this.po%this.ln]=c_s;this.po++;var c_p="";for(var c_n=0;c_n<this.ln;c_n++)c_p+=this.ar[this.po>this.ln?(this.po+c_n)%this.ln:c_n]+"\r\n";p(this.id,c_p);}}

